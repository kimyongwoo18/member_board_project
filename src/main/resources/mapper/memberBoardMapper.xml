<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="Member">
    <insert id="save" parameterType="memberDTO">
        insert into member_table2(memberEmail, memberPassword, memberName, memberMobile)
        values(#{memberEmail},#{memberPassword},#{memberName},#{memberMobile})
    </insert>

    <select id="findByEmail" resultType="memberDTO" >
        select * from member_table2 where memberEmail=#{memberEmail}
    </select>
</mapper>